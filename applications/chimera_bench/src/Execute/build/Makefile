%.o: $(VPATH)%.f95
	$(COMP) $< ${HDF5_FLIB}

%.o: $(VPATH)%.f
	$(COMP1) $< ${HDF5_FLIB}

RadHyd3D_mpi:	$(OBJFILES) ADIOS_IO_MODULE.o $(OBJFILES2)
	$(COMP) $(STD) $(OBJFILES) ADIOS_IO_MODULE.o $(OBJFILES2) ${HDF5_FLIB} $(SEGLDR2)
	$(SEGLDR) RadHyd3D_mpi $(RadHyd_LIB) $(OBJFILES) ADIOS_IO_MODULE.o $(OBJFILES2) ${HDF5_FLIB} $(SEGLDR2)

RadHyd3D_posix: $(OBJFILES) ADIOS_IO_MODULE_posix.o $(OBJFILES2)
	$(COMP) $(STD) $(OBJFILES) ADIOS_IO_MODULE_posix.o $(OBJFILES2) ${HDF5_FLIB} $(SEGLDR2)
	$(SEGLDR) RadHyd3D_posix  $(RadHyd_LIB) $(OBJFILES) ADIOS_IO_MODULE.o $(OBJFILES2) ${HDF5_FLIB} $(SEGLDR2)

debug:	
	@$(MAKE) WHAT=_RadHyd_GR COMP="$(COMP1) $(DBG)"

clean:
	rm -rf *.o *.mod RadHyd3D_mpi RadHyd3D_posix

