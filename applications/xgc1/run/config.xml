<adios-group name="diagnosis.bfield">
  <var name="nspace" type="integer"/>
  <var name="n_n" type="integer"/>
  <var name="values" gname="bfld" path="/node_data[0]" type="double" dimensions="3,n_n"/>
  <attribute name="nspace" path="/" var="nspace" type="integer"/>
  <attribute name="nnodes" path="/" var="n_n" type="integer"/>
  <attribute name="nnode_data" path="/" value="2" />
  <attribute name="XP_CLASS" path="/" value="Node_Data" />
  <attribute name="veclen" path="/node_data[0]/" value="3" />
  <attribute name="labels" path="/node_data[0]/" value="bfield" />
  <attribute name="units" path="/node_data[0]/" value="telsa" />
</adios-group>

<adios-group name="diagnosis.bfield.1">
  <var name="n_n" type="integer"/>
  <var name="values" gname="grid%psi" path="/node_data[1]" type="double" dimensions="n_n"/>
  <attribute name="veclen" path="/node_data[1]/" value="1" />
  <attribute name="labels" path="/node_data[1]/" value="poloidalflux" />
  <attribute name="units" path="/node_data[1]/" value="Tm^2" />
</adios-group>

<adios-group name="diag_pot.fieldp.header">
  <var name="nnode" type="integer"/>
  <var name="istep" type="integer"/>
  <var name="nspace" type="integer"/>
  <var name="nphi" type="integer"/>
  <attribute name="istep" path="/" var="istep" type="integer"/>
  <attribute name="nspace" path="/" var="nspace" type="integer"/>
  <attribute name="nnodes" path="/" var="nnode" type="integer"/>
  <attribute name="nnode_data" path="/" var="nphi" type="integer"/>
  <var name="values" gname="potential" path="/node_data[0]/" type="double" dimensions="nnode"/>

  <attribute name="labels" path="/node_data[0]/" value="potential_favg" />
  <attribute name="veclen" path="/node_data[0]/" value="nphi" />
  <attribute name="units" path="/node_data[0]/" value="V" />
</adios-group>

<adios-group name="diag_pot.fieldp" coordination-communicator="sml_pe_comm" >
  <var name="sml_pe_comm" type="integer"  />
  <var name="nnode" type="integer"/>
  <var name="nphi" type="integer"/>
  <gwrite src="if (sml_turb_efield==1) then"/>
  <var name="potential" gname="pot3d" type="double" dimensions="nnode,nphi"/>
  <gwrite src="endif"/>
</adios-group>

<adios-group name="fort.p">
  <var name="nnode" gname="grid%nnode" type="integer"/> 
  <var name="grid_x" gname="grid%x(:,:)" type="real*8" dimensions="2,nnode" copy-on-write="yes"/>
  <var name="psn_idensity" gname="psn%idensity(:,1)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="psn_dpot" gname="psn%dpot(:,1)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="psn_idensity0" gname="psn%idensity(:,1)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="psn_edensity0" gname="psn%dpot(:,1)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="psn_full" gname="psn%density0_full(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="psn_pot0" gname="psn%pot0(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <gwrite src="#ifdef XGC_DEBUG10"/>
  <var name="grid_node_vol" gname="grid%node_vol(:)*real(sml_nphi_total)" type="real*8" copy-on-write="yes" dimensions="nnode"/>
  <var name="grid_rtmp3" gname="grid%rtmp3(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="grid_rtmp4" gname="grid%rtmp4(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="grid_rtmp5" gname="grid%rtmp5(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="grid_rtmp6" gname="grid%rtmp6(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="grid_rtmp7" gname="grid%rtmp7(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <var name="grid_rtmp8" gname="grid%rtmp8(:)" type="real*8" dimensions="nnode" copy-on-write="yes"/>
  <gwrite src="#else"/>
  <var name="grid_node_vol" gname="grid%node_vol(:)*real(sml_nphi_total)" type="real*8" copy-on-write="yes" dimensions="nnode"/>
  <gwrite src="#endif"/>
</adios-group>

